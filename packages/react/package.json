    "dev": "start-storybook -p 6006",
    "start": "node scripts/start.js",
    "pretest": "npm run build",
    "test": "NODE_ENV=test jest --detectOpenHandles"
  },
  "license": "MIT",
  "jest": {
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testURL": "http://localhost/",
